/*place global styles here */
html,
body {
	@apply h-full;
}

@font-face {
	font-family: 'Space Grotesk';
	src: url('/fonts/SpaceGrotesk.ttf');
	font-display: swap;
}
.gradient-heading {
@apply bg-gradient-to-br from-pink-500 to-violet-500 text-transparent box-decoration-clone bg-clip-text text-3xl;  
}

.gradient-sub {
@apply bg-gradient-to-br from-pink-500 to-violet-500 text-transparent box-decoration-clone bg-clip-text text-xl
}
